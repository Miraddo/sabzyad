@charset "UTF-8";
:root {
  --primary-color: #10b981;
  --secondary-color: #059669;
  --accent-color: #34d399;
  --text-dark: #111827;
  --text-medium: #374151;
  --text-light: #6b7280;
  --text-muted: #9ca3af;
  --bg-white: #ffffff;
  --bg-light: #f9fafb;
  --bg-lighter: #f3f4f6;
  --border-color: #e5e7eb;
  --border-light: #f3f4f6;
  --success-color: #10b981;
  --warning-color: #f59e0b;
  --error-color: #ef4444;
  --info-color: #3b82f6;
  --border-radius: 12px;
  --border-radius-sm: 8px;
  --border-radius-lg: 16px;
  --box-shadow: 0 1px 3px 0 rgba(0, 0, 0, 0.05), 0 1px 2px 0 rgba(0, 0, 0, 0.05);
  --box-shadow-md: 0 4px 6px -1px rgba(0, 0, 0, 0.05), 0 2px 4px -1px rgba(0, 0, 0, 0.03);
  --box-shadow-lg: 0 10px 15px -3px rgba(0, 0, 0, 0.05), 0 4px 6px -2px rgba(0, 0, 0, 0.03);
  --font-family: 'Vazirmatn', -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif;
  --transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
  /* Code surfaces */
  --code-bg: #f3f4f6;
  /* light surface for code in light mode */
  --code-fg: #111827;
  /* dark text for readability */
}

/* Dark theme variables */
[data-theme="dark"] {
  --text-dark: #e5e7eb;
  --text-medium: #cbd5e1;
  --text-light: #94a3b8;
  --text-muted: #64748b;
  --bg-white: #0f172a;
  --bg-light: #0b1220;
  --bg-lighter: #111827;
  --border-color: #1f2937;
  --border-light: #111827;
  --box-shadow: 0 1px 3px 0 rgba(0, 0, 0, 0.6), 0 1px 2px 0 rgba(0, 0, 0, 0.4);
  --box-shadow-md: 0 4px 6px -1px rgba(0, 0, 0, 0.5), 0 2px 4px -1px rgba(0, 0, 0, 0.4);
  --box-shadow-lg: 0 10px 15px -3px rgba(0, 0, 0, 0.5), 0 4px 6px -2px rgba(0, 0, 0, 0.4);
  /* Code surfaces in dark */
  --code-bg: #0b1220;
  /* deep dark */
  --code-fg: #e5e7eb;
}

/* Base app surfaces pick from variables */
body {
  background: var(--bg-light);
  color: var(--text-dark);
}

/* Global horizontal overflow guard (site-wide) */
html, body {
  max-width: 100%;
  overflow-x: hidden;
}

/* Any direct layout containers should never overflow viewport */
.container, header, footer, main, section, nav {
  max-width: 100%;
  overflow-x: clip;
}

/* Media & tables responsive guarantees */
img, video, iframe {
  max-width: 100%;
  height: auto;
}

table {
  width: 100%;
  max-width: 100%;
}

pre {
  max-width: 100%;
}

/* Prevent long inline code / URLs from forcing overflow */
code, pre, kbd, samp {
  word-break: break-word;
  overflow-wrap: anywhere;
}

.site-header,
.site-footer,
.lesson-content,
.learn-sidebar,
.sidebar-card,
.course-sections {
  background: var(--bg-white);
  color: var(--text-dark);
}

/* Syntax highlighting wrappers */
.highlight,
.chroma {
  background: var(--code-bg);
  color: var(--code-fg);
}

/* Ensure all Chroma internals adopt code colors */
.highlight pre,
.highlight code,
.chroma pre,
.chroma code {
  background: transparent !important;
  color: inherit !important;
}

.chroma .lntable,
.chroma .lnt,
.chroma .ln,
.chroma td,
.chroma tr {
  background: transparent !important;
  color: inherit !important;
  border-color: var(--border-color) !important;
}

/* Prefer system dark if not set explicitly */
@media (prefers-color-scheme: dark) {
  :root:not([data-theme]) {
    --text-dark: #e5e7eb;
    --text-medium: #cbd5e1;
    --text-light: #94a3b8;
    --text-muted: #64748b;
    --bg-white: #0f172a;
    --bg-light: #0b1220;
    --bg-lighter: #111827;
    --border-color: #1f2937;
    --border-light: #111827;
    --box-shadow: 0 1px 3px 0 rgba(0, 0, 0, 0.6), 0 1px 2px 0 rgba(0, 0, 0, 0.4);
    --box-shadow-md: 0 4px 6px -1px rgba(0, 0, 0, 0.5), 0 2px 4px -1px rgba(0, 0, 0, 0.4);
    --box-shadow-lg: 0 10px 15px -3px rgba(0, 0, 0, 0.5), 0 4px 6px -2px rgba(0, 0, 0, 0.4);
    --code-bg: #0b1220;
    --code-fg: #e5e7eb;
  }
}

* {
  box-sizing: border-box;
  font-family: var(--font-family);
}

body {
  margin: 0;
  padding: 0;
  line-height: 1.6;
  color: var(--text-dark);
  font-family: var(--font-family);
}

body.no-scroll {
  /* lock scroll when mobile nav or overlays open */
  overflow: hidden;
  touch-action: none;
  overscroll-behavior: contain;
}

.learning-platform {
  line-height: 1.6;
  color: var(--text-dark);
}

.container {
  max-width: 1200px;
  margin: 0 auto;
  padding: 0 1rem;
}

.rtl {
  direction: rtl;
  text-align: right;
}

h1, h2, h3, h4, h5, h6 {
  font-weight: 600;
  line-height: 1.3;
  margin-bottom: 1rem;
}

h1 {
  font-size: 2.5rem;
}

h2 {
  font-size: 2rem;
}

h3 {
  font-size: 1.5rem;
}

p {
  margin-bottom: 1rem;
}

a {
  color: var(--primary-color);
  text-decoration: none;
  transition: color 0.3s ease;
}

a:hover {
  color: var(--secondary-color);
}

/* Fluid type scaling for small devices */
@media (max-width: 600px) {
  h1 {
    font-size: clamp(1.75rem, 6vw, 2.15rem);
  }
  h2 {
    font-size: clamp(1.4rem, 5.2vw, 1.75rem);
  }
  h3 {
    font-size: clamp(1.15rem, 4.5vw, 1.4rem);
  }
  p {
    font-size: .95rem;
  }
}

.text-center {
  text-align: center;
}

.text-left {
  text-align: left;
}

.text-right {
  text-align: right;
}

.mb-0 {
  margin-bottom: 0;
}

.mb-1 {
  margin-bottom: 0.5rem;
}

.mb-2 {
  margin-bottom: 1rem;
}

.mb-3 {
  margin-bottom: 1.5rem;
}

.mb-4 {
  margin-bottom: 2rem;
}

.mt-0 {
  margin-top: 0;
}

.mt-1 {
  margin-top: 0.5rem;
}

.mt-2 {
  margin-top: 1rem;
}

.mt-3 {
  margin-top: 1.5rem;
}

.mt-4 {
  margin-top: 2rem;
}

.p-0 {
  padding: 0;
}

.p-1 {
  padding: 0.5rem;
}

.p-2 {
  padding: 1rem;
}

.p-3 {
  padding: 1.5rem;
}

.p-4 {
  padding: 2rem;
