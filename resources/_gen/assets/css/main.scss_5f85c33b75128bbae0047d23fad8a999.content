/* Modern Courses Page Styles */
.course-list-page {
  min-height: 100vh;
}

/* Page Header */
.page-header {
  /* Use theme variables for background to support dark mode */
  background: var(--bg-lighter);
  padding: 4rem 0;
  text-align: center;
  position: relative;
  /* Keep a subtle decorative overlay; low opacity works in both themes */
}

.page-header::before {
  content: '';
  position: absolute;
  inset: 0;
  background: radial-gradient(circle at 20% 20%, rgba(16, 185, 129, 0.06), transparent 40%), radial-gradient(circle at 80% 30%, rgba(59, 130, 246, 0.05), transparent 40%);
}

.page-header .container {
  position: relative;
  z-index: 1;
}

.page-header h1 {
  font-size: clamp(2rem, 4vw, 3rem);
  font-weight: 800;
  color: var(--text-dark);
  margin-bottom: 1rem;
}

.page-header p {
  font-size: 1.125rem;
  color: var(--text-medium);
  max-width: 600px;
  margin: 0 auto 2rem;
}

.header-stats {
  display: flex;
  justify-content: center;
  gap: 2rem;
  margin-top: 2rem;
  flex-wrap: wrap;
}

.stat-card {
  /* Solid themed card to avoid washed look in dark mode */
  background: var(--bg-white);
  padding: 1.5rem;
  border-radius: var(--border-radius-lg);
  display: flex;
  align-items: center;
  gap: 1rem;
  min-width: 160px;
  border: 1px solid var(--border-light);
  box-shadow: var(--box-shadow);
}

.stat-card .stat-icon {
  font-size: 2rem;
}

.stat-card .stat-number {
  font-size: 1.5rem;
  font-weight: 800;
  color: var(--primary-color);
  line-height: 1;
}

.stat-card .stat-label {
  font-size: 0.875rem;
  color: var(--text-medium);
  font-weight: 500;
}

/* Filters Section */
.course-filters-section {
  background: var(--bg-white);
  padding: 2rem 0;
  border-bottom: 1px solid var(--border-light);
  position: sticky;
  top: 70px;
  z-index: 100;
}

.filters-container {
  display: flex;
  gap: 3rem;
  align-items: center;
  flex-wrap: wrap;
}

.filter-group {
  display: flex;
  align-items: center;
  gap: 1rem;
}

.filter-group label {
  font-weight: 600;
  color: var(--text-dark);
  font-size: 0.875rem;
  min-width: 80px;
}

.filter-options {
  display: flex;
  gap: 0.5rem;
  flex-wrap: wrap;
}

.filter-btn {
  padding: 0.5rem 1rem;
  border: 1px solid var(--border-color);
  border-radius: 25px;
  background: var(--bg-white);
  color: var(--text-medium);
  text-decoration: none;
  font-size: 0.875rem;
  font-weight: 500;
  transition: var(--transition);
  white-space: nowrap;
}

.filter-btn:hover {
  border-color: var(--primary-color);
  color: var(--primary-color);
  background: rgba(16, 185, 129, 0.05);
}

.filter-btn.active {
  background: var(--primary-color);
  color: white;
  border-color: var(--primary-color);
}

/* Meta text helpers on course cards (if used) */
.course-card .meta-item {
  color: var(--text-light);
}

[data-theme="dark"] .course-card .meta-item {
  color: var(--text-medium);
}

/* Courses Content */
.courses-content {
  padding: 3rem 0;
  background: var(--bg-light);
}

.courses-grid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(350px, 1fr));
  gap: 2rem;
}

/* Enhanced Course Card */
.course-card {
  background: var(--bg-white);
  border-radius: var(--border-radius-lg);
  overflow: hidden;
  box-shadow: 0 4px 6px -1px rgba(0, 0, 0, 0.05), 0 2px 4px -1px rgba(0, 0, 0, 0.03);
  transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
  border: 1px solid var(--border-light);
  position: relative;
  display: flex;
  flex-direction: column;
  height: 100%;
}

.course-card:hover {
  transform: translateY(-8px);
  box-shadow: 0 20px 25px -5px rgba(0, 0, 0, 0.08), 0 10px 10px -5px rgba(0, 0, 0, 0.04);
  border-color: var(--primary-color);
}

.course-card:hover .course-image img {
  transform: scale(1.05);
}

.course-card:hover .course-title a {
  color: var(--primary-color);
}

.course-image {
  position: relative;
  height: 200px;
  overflow: hidden;
  background: var(--bg-lighter);
}

.course-image img {
  width: 100%;
  height: 100%;
  object-fit: cover;
  transition: transform 0.3s ease;
}

.course-image .image-placeholder {
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  background: var(--bg-lighter);
}

.course-image .image-placeholder .placeholder-icon {
  font-size: 3rem;
  opacity: 0.4;
  color: var(--text-muted);
}

.course-level {
  position: absolute;
  top: 12px;
  right: 12px;
  background: linear-gradient(135deg, var(--primary-color), var(--secondary-color));
  color: white;
  padding: 6px 12px;
  border-radius: 20px;
  font-size: 0.75rem;
  font-weight: 600;
  box-shadow: 0 2px 8px rgba(16, 185, 129, 0.3);
  backdrop-filter: blur(10px);
}

.course-duration {
  position: absolute;
  bottom: 12px;
  left: 12px;
  background: rgba(0, 0, 0, 0.8);
  color: white;
  padding: 6px 10px;
  border-radius: var(--border-radius-sm);
  font-size: 0.75rem;
  font-weight: 500;
  display: flex;
  align-items: center;
  gap: 4px;
  backdrop-filter: blur(10px);
}

.course-content {
  padding: 24px;
  flex: 1;
  display: flex;
  flex-direction: column;
}

.course-title {
  margin-bottom: 12px;
}

.course-title a {
  font-size: 1.25rem;
  font-weight: 700;
  color: var(--text-dark);
  text-decoration: none;
  line-height: 1.3;
  transition: color 0.2s ease;
  display: block;
}

.course-title a:hover {
  color: var(--primary-color);
}

.course-description {
  color: var(--text-medium);
  line-height: 1.6;
  margin-bottom: 24px;
  font-size: 0.95rem;
  flex: 1;
  display: -webkit-box;
  -webkit-line-clamp: 3;
  line-clamp: 3;
  -webkit-box-orient: vertical;
  overflow: hidden;
}

.course-footer {
  padding: 0 24px 24px;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  margin-top: auto;
}

/* Modern Course Button */
.course-btn {
  display: inline-flex;
  align-items: center;
  gap: 8px;
  background: linear-gradient(135deg, var(--primary-color) 0%, var(--secondary-color) 100%);
  color: white;
  padding: 12px 24px;
  border-radius: 12px;
  text-decoration: none;
  font-weight: 600;
  font-size: 0.9rem;
  transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
  border: none;
  cursor: pointer;
  box-shadow: 0 3px 12px rgba(16, 185, 129, 0.25);
  position: relative;
  overflow: hidden;
  min-width: 140px;
  justify-content: center;
}

.course-btn::before {
  content: '';
  position: absolute;
  top: 0;
  left: -100%;
  width: 100%;
  height: 100%;
  background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.2), transparent);
  transition: left 0.6s ease;
}

.course-btn:hover {
  transform: translateY(-2px);
  box-shadow: 0 6px 20px rgba(16, 185, 129, 0.35);
  background: linear-gradient(135deg, var(--secondary-color) 0%, #047857 100%);
}

.course-btn:hover::before {
  left: 100%;
}

.course-btn:hover .btn-icon {
  transform: translateX(3px);
}

.course-btn:active {
  transform: translateY(-1px) scale(1.01);
  box-shadow: 0 4px 15px rgba(16, 185, 129, 0.3);
}

.course-btn .btn-text {
  font-family: var(--font-family);
  letter-spacing: 0.025em;
}

.course-btn .btn-icon {
  transition: transform 0.3s ease;
  stroke-width: 2.5;
}

/* No Courses State */
.no-courses {
  text-align: center;
  padding: 4rem 2rem;
  color: var(--text-muted);
}

.no-courses .no-courses-icon {
  font-size: 4rem;
  margin-bottom: 1rem;
  opacity: 0.5;
}

.no-courses h3 {
  font-size: 1.5rem;
  margin-bottom: 0.5rem;
  color: var(--text-medium);
}

.no-courses p {
  font-size: 1rem;
  color: var(--text-muted);
}

/* Responsive Design */
@media (max-width: 768px) {
  .page-header {
    padding: 3rem 0;
  }
  .header-stats {
    gap: 1rem;
  }
  .stat-card {
    min-width: 140px;
    padding: 1rem;
    flex-direction: column;
    text-align: center;
    gap: 0.5rem;
  }
  .filters-container {
    flex-direction: column;
    gap: 1.5rem;
    align-items: stretch;
  }
  .filter-group {
    flex-direction: column;
    align-items: stretch;
    gap: 0.75rem;
  }
  .filter-group label {
    min-width: auto;
  }
  .course-filters-section {
    position: static;
    padding: 1.5rem 0;
  }
  .courses-grid {
    grid-template-columns: 1fr;
    gap: 1.5rem;
  }
}

.courses-page {
  padding: 2rem 0 4rem;
  min-height: 80vh;
}

.page-header {
  text-align: center;
  margin-bottom: 3rem;
}

.page-header .page-title {
  font-size: 2.5rem;
  font-weight: 700;
  color: var(--text-dark);
  margin-bottom: 1rem;
}

.page-header .page-description {
  font-size: 1.2rem;
  color: var(--text-light);
  max-width: 600px;
  margin: 0 auto;
  line-height: 1.6;
}

.courses-filter {
  margin-bottom: 3rem;
}

.courses-filter .filter-controls {
  display: flex;
  flex-wrap: wrap;
  gap: 1rem;
  align-items: center;
  justify-content: space-between;
}

.courses-filter .search-box {
  flex: 1;
  min-width: 300px;
}

.courses-filter .search-box input {
  width: 100%;
  padding: 0.75rem 1rem;
  border: 1px solid var(--border-color);
  border-radius: var(--border-radius);
  font-size: 1rem;
  transition: border-color 0.3s ease;
}

.courses-filter .search-box input:focus {
  outline: none;
  border-color: var(--primary-color);
}

.courses-filter .search-box input::placeholder {
  color: var(--text-light);
}

.courses-filter .filter-buttons {
  display: flex;
  gap: 0.5rem;
  flex-wrap: wrap;
}

.courses-filter .filter-btn {
  padding: 0.5rem 1rem;
  border: 1px solid var(--border-color);
  background: white;
  color: var(--text-dark);
  border-radius: var(--border-radius);
  cursor: pointer;
  transition: all 0.3s ease;
  font-size: 0.9rem;
}

.courses-filter .filter-btn:hover, .courses-filter .filter-btn.active {
  background: var(--primary-color);
  color: white;
  border-color: var(--primary-color);
}

.courses-container .courses-grid {
  display: grid;
  grid-template-columns: repeat(auto-fill, minmax(350px, 1fr));
  gap: 2rem;
}

.no-courses {
  text-align: center;
  padding: 4rem 2rem;
}

.no-courses .no-courses-icon {
  font-size: 4rem;
  margin-bottom: 1rem;
  display: block;
}

.no-courses h3 {
  font-size: 1.5rem;
  margin-bottom: 0.5rem;
  color: var(--text-dark);
}

.no-courses p {
  color: var(--text-light);
}

@media (max-width: 768px) {
  .filter-controls {
    flex-direction: column;
    align-items: stretch;
  }
  .search-box {
    min-width: auto;
  }
  .filter-buttons {
    justify-content: center;
  }
  .courses-grid {
    grid-template-columns: 1fr;
  }
  .page-title {
    font-size: 2rem;
  }
}

@media (max-width: 480px) {
  .page-title {
    font-size: 1.8rem;
  }
}

.course-overview {
  min-height: 100vh;
  background: var(--bg-light);
}

.course-hero {
  background: linear-gradient(135deg, #4f46e5 0%, #7c3aed 100%);
  color: white;
  padding: 80px 0;
  min-height: 500px;
  display: flex;
  align-items: center;
}

.course-hero .container {
  width: 100%;
  max-width: 1200px;
  margin: 0 auto;
  padding: 0 20px;
}

.course-hero-content {
  display: grid;
  grid-template-columns: 2fr 1fr;
  gap: 60px;
  align-items: center;
  width: 100%;
}

.course-badge {
  display: inline-block;
  background: rgba(255, 255, 255, 0.15);
  padding: 8px 16px;
  border-radius: 20px;
  font-size: 14px;
  margin-bottom: 20px;
  backdrop-filter: blur(10px);
  border: 1px solid rgba(255, 255, 255, 0.2);
}

.course-title {
  font-size: 2.8rem;
  font-weight: 700;
  margin-bottom: 20px;
  line-height: 1.2;
  color: #ffffff;
}

.course-description {
  font-size: 1.1rem;
  margin-bottom: 30px;
  opacity: 0.95;
  line-height: 1.6;
  color: #f8fafc;
}

.course-meta {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
  gap: 15px;
  margin-bottom: 30px;
}

.meta-item {
  display: flex;
  align-items: center;
  gap: 10px;
  font-size: 14px;
  color: #f1f5f9;
  /* default on purple hero */
  background: rgba(255, 255, 255, 0.12);
  padding: 8px 12px;
  border-radius: 14px;
  border: 1px solid rgba(255, 255, 255, 0.22);
}

.meta-item i {
  color: #fbbf24;
  font-size: 16px;
}

/* Dark mode readability for meta items that appear outside hero surfaces */
[data-theme="dark"] .course-content .meta-item,
[data-theme="dark"] .course-sections .meta-item {
  color: var(--text-medium);
}

/* Ensure icons/links inside meta inherit color */
.course-meta .meta-item a {
  color: inherit;
}

.course-meta .meta-item i,
.course-meta .meta-item svg {
  color: inherit;
  opacity: .95;
}

.course-price {
  margin-bottom: 30px;
}

.price {
  font-size: 1.8rem;
  font-weight: 700;
  color: var(--primary-color);
  background: rgba(16, 185, 129, 0.15);
  padding: 8px 16px;
  border-radius: 8px;
  display: inline-block;
}

.course-actions {
  display: flex;
  gap: 15px;
}

.course-image {
  text-align: center;
}

.course-image img {
  max-width: 100%;
  border-radius: 12px;
  box-shadow: 0 20px 40px rgba(0, 0, 0, 0.2);
}

.course-placeholder {
  width: 300px;
  height: 200px;
  background: rgba(255, 255, 255, 0.1);
  border-radius: 12px;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 4rem;
  color: rgba(255, 255, 255, 0.5);
}

.course-content {
  padding: 80px 0;
  background: var(--bg-light);
}

.course-content .container {
  display: grid;
  grid-template-columns: 2fr 1fr;
  gap: 60px;
  max-width: 1200px;
  margin: 0 auto;
  padding: 0 20px;
}

.course-sections {
  background: var(--bg-white);
  border-radius: 12px;
  padding: 40px;
  box-shadow: var(--box-shadow);
}

.course-section {
  margin-bottom: 50px;
}

.course-section:last-child {
  margin-bottom: 0;
}

.course-section h2 {
  font-size: 2rem;
  margin-bottom: 30px;
  color: var(--text-dark);
  border-bottom: 2px solid var(--border-light);
  padding-bottom: 15px;
}

.learning-outcomes,
.prerequisites {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
  gap: 20px;
}

.outcome-item,
.prerequisite-item {
  display: flex;
  align-items: center;
  gap: 15px;
  padding: 20px;
  background: var(--bg-light);
  border-radius: 8px;
  border-left: 4px solid var(--primary-color);
  transition: all 0.3s ease;
}

.outcome-item:hover,
.prerequisite-item:hover {
  background: var(--bg-lighter);
  transform: translateX(-5px);
  box-shadow: var(--box-shadow-md);
}

.outcome-item i,
.prerequisite-item i {
  color: var(--primary-color);
  font-size: 1.2rem;
  flex-shrink: 0;
}

.outcome-item span,
.prerequisite-item span {
  color: var(--text-medium);
  font-weight: 500;
}

.curriculum-preview {
  background: linear-gradient(135deg, var(--bg-light) 0%, var(--border-light) 100%);
  padding: 40px;
  border-radius: 16px;
  text-align: center;
  border: 1px solid var(--border-light);
}

.curriculum-info p {
  font-size: 1.1rem;
  color: var(--text-medium);
  margin-bottom: 30px;
  font-weight: 500;
}

.curriculum-highlights {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
  gap: 20px;
  margin-bottom: 40px;
}

.highlight-item {
  display: flex;
  align-items: center;
  justify-content: center;
  gap: 10px;
  padding: 20px;
  background: var(--bg-white);
  border-radius: 12px;
  box-shadow: var(--box-shadow);
  transition: all 0.3s ease;
}

.highlight-item:hover {
  transform: translateY(-4px);
  box-shadow: 0 8px 20px rgba(0, 0, 0, 0.1);
}

.highlight-item i {
  color: #4f46e5;
  font-size: 1.3rem;
}

.highlight-item span {
  color: var(--text-medium);
  font-weight: 600;
}

.start-learning {
  margin-top: 20px;
}

.start-learning .btn {
  font-size: 1.1rem;
  padding: 20px 40px;
  border-radius: 12px;
}

.course-description-full {
  font-size: 1.1rem;
  line-height: 1.8;
  color: var(--text-medium);
}

.course-description-full h2 {
  color: var(--text-dark);
  font-size: 1.5rem;
  margin: 30px 0 15px 0;
  font-weight: 600;
}

.course-description-full h3 {
  color: var(--text-medium);
  font-size: 1.3rem;
  margin: 25px 0 12px 0;
  font-weight: 600;
}

.course-description-full h4 {
  color: var(--text-light);
  font-size: 1.1rem;
  margin: 20px 0 10px 0;
  font-weight: 600;
}

.course-description-full p {
  margin-bottom: 20px;
  color: var(--text-medium);
}

.course-description-full ul, .course-description-full ol {
  margin: 20px 0;
  padding-right: 20px;
}

.course-description-full li {
  margin-bottom: 8px;
  color: var(--text-medium);
}

.course-description-full strong {
  color: var(--text-dark);
  font-weight: 600;
}

.course-sidebar > * + * {
  margin-top: 30px;
}

.sidebar-card {
  background: var(--bg-white);
  padding: 30px;
  border-radius: 12px;
  box-shadow: var(--box-shadow);
  margin-bottom: 30px;
  border: 1px solid var(--border-light);
}

.sidebar-card h3 {
  margin-bottom: 20px;
  color: var(--text-dark);
  font-size: 1.2rem;
  font-weight: 600;
}

.course-stats > * + * {
  margin-top: 15px;
}

.stat-item {
  display: flex;
  justify-content: space-between;
  padding-bottom: 15px;
  border-bottom: 1px solid var(--border-light);
}

.stat-item:last-child {
  border-bottom: none;
  padding-bottom: 0;
}

.stat-label {
  color: var(--text-light);
  font-size: 14px;
}

.stat-value {
  font-weight: 600;
  color: var(--text-dark);
  font-size: 14px;
}

.course-tags {
  display: flex;
  flex-wrap: wrap;
  gap: 10px;
}

.tag {
  background: var(--bg-lighter);
  color: var(--text-medium);
  padding: 6px 12px;
  border-radius: 6px;
  font-size: 12px;
  font-weight: 500;
}

@media (max-width: 768px) {
  .course-hero {
    padding: 60px 0;
    min-height: auto;
  }
  /* Contributors mini list */
  .contributors-list {
    display: flex;
    flex-wrap: wrap;
    gap: .4rem;
  }
  .contributors-list .contrib-mini {
    display: inline-flex;
    align-items: center;
    padding: 0;
    border: none;
    border-radius: 50%;
  }
  .contributors-list .contrib-mini img {
    width: 32px;
    height: 32px;
    border-radius: 50%;
    border: 1px solid var(--border-color);
    object-fit: cover;
  }
  .course-hero-content {
    grid-template-columns: 1fr;
    text-align: center;
    gap: 40px;
  }
  .course-content .container {
    grid-template-columns: 1fr;
    gap: 40px;
  }
  .course-title {
    font-size: 2rem;
  }
  .course-meta {
    grid-template-columns: 1fr;
    gap: 10px;
  }
  .course-actions {
    flex-direction: column;
    gap: 15px;
  }
  .learning-outcomes, .prerequisites {
    grid-template-columns: 1fr;
  }
  .curriculum-highlights {
    grid-template-columns: repeat(2, 1fr);
    gap: 15px;
  }
  .course-sections {
    padding: 20px;
  }
}

/* References sidebar card */
.references-card .ref-section {
  margin-top: 1rem;
}

.references-card .ref-section-title {
  margin-bottom: .5rem;
}

.references-card .ref-badge {
  display: inline-flex;
  align-items: center;
  gap: .4rem;
  padding: .3rem .6rem;
  border-radius: 999px;
  font-size: .8rem;
  font-weight: 700;
  border: 1px solid var(--border-color);
}

.references-card .ref-badge.free {
  background: rgba(16, 185, 129, 0.12);
  color: var(--text-dark);
}

.references-card .ref-badge.paid {
  background: rgba(245, 158, 11, 0.12);
  color: var(--text-dark);
}

.references-card .references-list {
  list-style: none;
  padding: 0;
  margin: .5rem 0 0;
}

.references-card .reference-item {
  display: grid;
  grid-template-columns: auto 1fr auto;
  align-items: center;
  gap: .5rem .75rem;
  padding: .5rem 0;
  border-bottom: 1px dashed var(--border-color);
}

.references-card .reference-item:last-child {
  border-bottom: 0;
}

.references-card .ref-leading {
  width: 1.25rem;
  display: inline-flex;
  justify-content: center;
  color: var(--text-light);
}

.references-card .ref-link {
  color: var(--text-dark);
  text-decoration: none;
}

.references-card .ref-link:hover {
  color: var(--primary-color);
}

.references-card .ref-meta-wrap {
  display: inline-flex;
  align-items: center;
  gap: .4rem;
}

.references-card .ref-lang {
  background: var(--bg-lighter);
