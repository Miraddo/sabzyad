  position: fixed;
  bottom: 2rem;
  right: 2rem;
  width: 3rem;
  height: 3rem;
  background: var(--primary-color);
  color: white;
  border: none;
  border-radius: 50%;
  cursor: pointer;
  font-size: 1.2rem;
  opacity: 0;
  visibility: hidden;
  transition: all 0.3s ease;
  z-index: 1000;
}

.back-to-top.show {
  opacity: 1;
  visibility: visible;
}

.back-to-top:hover {
  background: var(--secondary-color);
  transform: translateY(-3px);
}

.rtl .back-to-top {
  right: auto;
  left: 2rem;
}

.rtl .buttons.is-right {
  justify-content: flex-start;
}

.course-card {
  background: white;
  border-radius: 16px;
  overflow: hidden;
  box-shadow: 0 4px 20px rgba(0, 0, 0, 0.08);
  transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
  border: 1px solid rgba(229, 231, 235, 0.8);
}

.course-card:hover {
  transform: translateY(-4px);
  box-shadow: 0 8px 30px rgba(0, 0, 0, 0.12);
  border-color: rgba(16, 185, 129, 0.3);
}

.course-card .course-image {
  position: relative;
  height: 200px;
  overflow: hidden;
  background: linear-gradient(135deg, #f3f4f6 0%, #e5e7eb 100%);
}

.course-card .course-image img {
  width: 100%;
  height: 100%;
  object-fit: cover;
  transition: transform 0.3s ease;
}

.course-card .course-image:not(:has(img))::before, .course-card .course-image:has(img[src=""])::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: linear-gradient(45deg, #f9fafb 25%, transparent 25%, transparent 75%, #f9fafb 75%, #f9fafb);
  background-size: 20px 20px;
  animation: movePattern 2s linear infinite;
}

.course-card .course-image:not(:has(img))::after, .course-card .course-image:has(img[src=""])::after {
  content: '📚';
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  font-size: 3rem;
  opacity: 0.6;
  z-index: 1;
}

.course-card .course-image:hover img {
  transform: scale(1.05);
}

.course-card .course-status {
  position: absolute;
  top: 16px;
  right: 16px;
  padding: 6px 12px;
  border-radius: 20px;
  font-size: 0.75rem;
  font-weight: 600;
  text-transform: uppercase;
  letter-spacing: 0.5px;
}

.course-card .course-content {
  padding: 24px;
}

.course-card .course-content .course-title {
  font-size: 1.25rem;
  font-weight: 700;
  color: var(--text-primary);
  margin-bottom: 12px;
  line-height: 1.4;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
}

.course-card .course-content .course-description {
  color: var(--text-secondary);
  font-size: 0.875rem;
  line-height: 1.6;
  margin-bottom: 20px;
  display: -webkit-box;
  -webkit-line-clamp: 3;
  line-clamp: 3;
  -webkit-box-orient: vertical;
  overflow: hidden;
}

.course-card .course-content .course-stats {
  display: flex;
  justify-content: space-between;
  margin-bottom: 20px;
  padding: 16px;
  background: rgba(16, 185, 129, 0.05);
  border-radius: 12px;
  border: 1px solid rgba(16, 185, 129, 0.1);
}

.course-card .course-content .course-stats .stat {
  text-align: center;
}

.course-card .course-content .course-stats .stat .value {
  font-weight: 700;
  color: var(--primary-color);
  font-size: 1.1rem;
  display: block;
}

.course-card .course-content .course-stats .stat .label {
  font-size: 0.75rem;
  color: var(--text-secondary);
  margin-top: 4px;
}

.course-card .course-content .course-actions {
  display: flex;
  gap: 12px;
}

.course-card .course-content .course-actions .btn {
  flex: 1;
  padding: 12px 16px;
  border-radius: 12px;
  font-weight: 600;
  text-decoration: none;
  text-align: center;
  transition: all 0.3s ease;
  font-size: 0.875rem;
}

.course-card .course-content .course-actions .btn.btn-primary {
  background: linear-gradient(135deg, var(--primary-color), #059669);
  color: white;
  border: none;
}

.course-card .course-content .course-actions .btn.btn-primary:hover {
  transform: translateY(-1px);
  box-shadow: 0 4px 12px rgba(16, 185, 129, 0.3);
}

.course-card .course-content .course-actions .btn.btn-secondary {
  background: white;
  color: var(--primary-color);
  border: 2px solid var(--primary-color);
}

.course-card .course-content .course-actions .btn.btn-secondary:hover {
  background: var(--primary-color);
  color: white;
}

.courses-grid {
  display: grid;
  grid-template-columns: repeat(auto-fill, minmax(350px, 1fr));
  gap: 24px;
  margin-top: 32px;
}

@media (max-width: 768px) {
  .courses-grid {
    grid-template-columns: 1fr;
    gap: 16px;
  }
}

@keyframes movePattern {
  0% {
    background-position: 0 0;
  }
  100% {
    background-position: 20px 20px;
  }
}

.featured-course {
  background: linear-gradient(135deg, rgba(16, 185, 129, 0.1), rgba(16, 185, 129, 0.05));
  border: 2px solid rgba(16, 185, 129, 0.2);
}

.blog-card {
  background: var(--bg-white);
  border-radius: 20px;
  overflow: hidden;
  box-shadow: 0 4px 20px rgba(0, 0, 0, 0.08);
  transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
  border: 1px solid var(--border-color);
}

.blog-card:hover {
  transform: translateY(-8px);
  box-shadow: 0 20px 40px rgba(0, 0, 0, 0.12);
  border-color: rgba(16, 185, 129, 0.3);
}

.blog-card .blog-image {
  position: relative;
  /* Uniform fixed cover height for consistent sizing */
  height: 220px;
  max-height: 220px;
  overflow: hidden;
  background: linear-gradient(135deg, var(--bg-lighter) 0%, var(--bg-light) 100%);
  border-bottom: 1px solid var(--border-color);
}

.blog-card .blog-image img {
  width: 100%;
  height: 100%;
  object-fit: cover;
  object-position: center;
  transition: transform 0.3s ease;
}

.blog-card .blog-image:empty::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: linear-gradient(45deg, #f9fafb 25%, transparent 25%, transparent 75%, #f9fafb 75%, #f9fafb);
  background-size: 20px 20px;
  animation: movePattern 2s linear infinite;
}

.blog-card .blog-image:empty::after {
  content: '📝';
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  font-size: 3rem;
  opacity: 0.6;
  z-index: 1;
  color: var(--primary-color);
}

.blog-card .blog-image:hover img {
  transform: scale(1.05);
}

.blog-card .blog-content {
  padding: 1.25rem 1.25rem 1.5rem;
  background: var(--bg-white);
}

.blog-card .blog-content .blog-title {
  font-size: 1.15rem;
  font-weight: 800;
  color: var(--text-dark) !important;
  margin-bottom: .5rem;
  line-height: 1.3;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
}

.blog-card .blog-content .blog-title a {
  color: var(--text-dark) !important;
  text-decoration: none;
  transition: color 0.3s ease;
}

.blog-card .blog-content .blog-title a:hover {
  color: var(--primary-color) !important;
}

.blog-card .blog-content .blog-excerpt {
  color: var(--text-medium) !important;
  font-size: 0.95rem;
  line-height: 1.65;
  margin-bottom: 1rem;
  display: -webkit-box;
  -webkit-line-clamp: 3;
  line-clamp: 3;
  -webkit-box-orient: vertical;
  overflow: hidden;
}

.blog-card .blog-content .blog-meta {
  display: flex;
  align-items: center;
  gap: 0.75rem;
  margin-bottom: 1rem;
  color: var(--text-light) !important;
  font-size: 0.875rem;
}

.blog-card .blog-content .blog-meta time {
  color: var(--text-light) !important;
}

.blog-card .blog-content .blog-meta::before {
  content: '📅';
  font-size: 0.875rem;
  opacity: 0.7;
}

.blog-card .blog-content .read-more {
  display: inline-flex;
  align-items: center;
  gap: 0.5rem;
  color: var(--primary-color) !important;
  text-decoration: none;
  font-weight: 600;
  font-size: 0.85rem;
  padding: 0.45rem 0.9rem;
  border: 1px solid var(--primary-color);
  border-radius: 20px;
  background: var(--bg-white);
  transition: all 0.3s ease;
}

.blog-card .blog-content .read-more::after {
  content: '←';
  transition: transform 0.3s ease;
  font-size: 0.75rem;
}

.blog-card .blog-content .read-more:hover {
  background: var(--primary-color) !important;
  color: #fff !important;
  transform: translateX(3px);
  box-shadow: 0 2px 8px rgba(16, 185, 129, 0.25);
}

.blog-card .blog-content .read-more:hover::after {
  transform: translateX(2px);
}

/* Dark mode refinements for blog cards */
[data-theme="dark"] .blog-card {
  box-shadow: 0 4px 20px rgba(0, 0, 0, 0.6);
  border-color: var(--border-color);
}

[data-theme="dark"] .blog-card .blog-image {
  background: linear-gradient(135deg, var(--bg-lighter) 0%, var(--bg-light) 100%);
}

[data-theme="dark"] .blog-card .blog-content {
  background: var(--bg-white);
}

[data-theme="dark"] .blog-card .read-more {
  background: var(--bg-white);
  color: var(--primary-color) !important;
}

[data-theme="dark"] .blog-card .read-more:hover {
  background: var(--primary-color) !important;
  color: #fff !important;
}

/* Responsive tweak: slightly shorter covers on small screens */
@media (max-width: 640px) {
  .blog-card .blog-image {
    height: 190px;
    max-height: 190px;
  }
}

.progress-card .progress-bar {
  width: 100%;
  height: 8px;
  background: rgba(16, 185, 129, 0.1);
  border-radius: 4px;
  overflow: hidden;
  margin-top: 12px;
}

.progress-card .progress-bar .progress-fill {
  height: 100%;
  background: linear-gradient(90deg, var(--primary-color), #059669);
  border-radius: 4px;
  transition: width 0.3s ease;
}

.progress-card .progress-text {
  display: flex;
  justify-content: space-between;
  margin-top: 8px;
  font-size: 0.75rem;
  color: var(--text-secondary);
}

@keyframes float {
  0%, 100% {
    transform: translateY(0px);
  }
  50% {
    transform: translateY(-10px);
  }
}

/* Simple global rule: ensure blog image fills container width */
.blog-image img {
  width: 100%;
}

/* Contributors grid */
.contributors {
  margin-top: 1.5rem;
}

.contributors .section-title {
  font-size: 1.25rem;
  margin: 0 0 .75rem;
}

.contributors-grid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(220px, 1fr));
  gap: .75rem;
}

.contrib-card {
  display: flex;
  align-items: center;
  gap: .75rem;
  padding: .75rem;
  border: 1px solid var(--border-color);
  border-radius: 14px;
  background: var(--bg-white);
  text-decoration: none;
  color: inherit;
  transition: var(--transition);
}

.contrib-card:hover {
  transform: translateY(-2px);
  box-shadow: var(--box-shadow);
}

.contrib-avatar {
  width: 28px;
  height: 28px;
  border-radius: 50%;
  border: 1px solid var(--border-color);
  object-fit: cover;
}

.contrib-meta {
  display: flex;
  flex-direction: column;
}

.contrib-name {
  font-weight: 700;
}

.contrib-role {
  color: var(--text-light);
  font-size: .9rem;
}

.contrib-card .arrow {
  margin-inline-start: auto;
  color: var(--text-muted);
}

/* Avatar-only contributor chip (global) */
.contrib-mini {
  display: inline-flex;
  align-items: center;
  justify-content: center;
  padding: 0;
  border: none;
  border-radius: 50%;
  text-decoration: none;
  color: inherit;
}

.contrib-mini img {
  width: 32px;
  height: 32px;
  border-radius: 50%;
  border: 1px solid var(--border-color);
  object-fit: cover;
  display: block;
}

/* Contributors list container (global) */
.contributors-list {
  display: flex;
  flex-wrap: wrap;
  gap: .4rem;
}

/* LearnDash-style Learning Interface */
.learndash-style {
  background: #f8fafc;
  min-height: 100vh;
}

.learning-grid {
  display: grid;
  grid-template-columns: 320px 1fr;
  gap: 0;
  min-height: calc(100vh - 200px);
  padding: 1rem 1rem 1rem;
}

/* Learning Sidebar */
.learning-sidebar {
  background: white;
  border-right: 1px solid #e5e7eb;
  padding: 0;
  position: sticky;
  top: 80px;
  height: calc(100vh - 80px);
  overflow-y: auto;
  box-shadow: 2px 0 8px rgba(0, 0, 0, 0.1);
}

.learning-sidebar .sidebar-card {
  margin: 0;
  border-radius: 0;
  border: none;
  border-bottom: 1px solid #e5e7eb;
  padding: 20px;
}

.learning-sidebar .sidebar-card:last-child {
  border-bottom: none;
}

/* Progress Card */
.progress-card .progress-stats {
  display: flex;
  align-items: center;
  gap: 16px;
  margin-top: 16px;
}

.progress-circle {
  position: relative;
  width: 80px;
  height: 80px;
}

.circle-progress {
  width: 80px;
  height: 80px;
  border-radius: 50%;
  background: conic-gradient(#10b981 0deg, #e5e7eb 0deg);
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
}

.circle-progress::before {
  content: '';
  position: absolute;
  width: 60px;
  height: 60px;
  border-radius: 50%;
  background: white;
  z-index: 1;
}

.progress-text {
  position: relative;
  z-index: 2;
  font-weight: 600;
  color: #1f2937;
  font-size: 14px;
}

.progress-info {
  flex: 1;
}

.progress-info p {
  margin: 0;
  font-size: 13px;
  color: #6b7280;
}

.time-remaining {
  font-weight: 500;
  color: #10b981;
}

/* Curriculum Navigation */
.curriculum-nav .curriculum-tree {
  margin-top: 16px;
}

.module-nav {
  border-bottom: 1px solid #f3f4f6;
}

.module-nav:last-child {
  border-bottom: none;
}

.module-nav-header {
  display: flex;
  align-items: center;
  gap: 8px;
  padding: 12px 0;
  cursor: pointer;
  transition: all 0.2s ease;
}

.module-nav-header:hover {
  background: #f9fafb;
  margin: 0 -20px;
  padding-left: 20px;
  padding-right: 20px;
}

.module-chevron {
  transition: transform 0.2s ease;
  color: #9ca3af;
}

.module-nav-title {
  flex: 1;
  font-size: 14px;
  font-weight: 500;
  color: #374151;
}

.module-nav-count {
  background: #f3f4f6;
  color: #6b7280;
  font-size: 12px;
  padding: 2px 8px;
  border-radius: 12px;
  font-weight: 500;
}

.lessons-nav {
  padding-left: 24px;
  padding-bottom: 8px;
}

.lesson-nav-item {
  display: flex;
  align-items: center;
  gap: 10px;
  padding: 8px 12px;
  margin: 4px 0;
  border-radius: 6px;
  cursor: pointer;
  transition: all 0.2s ease;
}

.lesson-nav-item:hover {
  background: #f3f4f6;
}

.lesson-nav-item.active {
  background: #10b981;
  color: white;
}

.lesson-nav-item.active .lesson-nav-title,
.lesson-nav-item.active .lesson-nav-duration {
  color: white;
}

.lesson-status .lesson-icon {
  color: #9ca3af;
}

.lesson-nav-item.active .lesson-status .lesson-icon {
  color: white;
}

.lesson-nav-content {
  flex: 1;
  min-width: 0;
}

.lesson-nav-title {
  display: block;
  font-size: 13px;
  font-weight: 500;
  color: #374151;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.lesson-nav-duration {
  font-size: 11px;
  color: #9ca3af;
  font-weight: 400;
}

/* Quick Tools */
.tools-card .quick-tools {
  display: flex;
  flex-direction: column;
  gap: 8px;
  margin-top: 16px;
}

.tool-btn {
  display: flex;
  align-items: center;
  gap: 10px;
  padding: 10px 12px;
  border: 1px solid #e5e7eb;
  background: white;
  border-radius: 6px;
  cursor: pointer;
  transition: all 0.2s ease;
  font-size: 13px;
  color: #6b7280;
  text-align: left;
}

.tool-btn:hover {
  background: #f9fafb;
  border-color: #d1d5db;
}

.tool-btn.bookmarked {
  color: #10b981;
  border-color: #10b981;
}

/* Main Learning Content */
.learning-main {
  background: white;
  display: flex;
  flex-direction: column;
}

.content-viewer {
  flex: 1;
  display: flex;
  flex-direction: column;
}

/* Content Header */
.content-header {
  background: white;
  border-bottom: 1px solid #e5e7eb;
  padding: 20px 30px;
  position: sticky;
  top: 0;
  z-index: 10;
}

.content-nav {
  display: flex;
  align-items: center;
  justify-content: space-between;
  gap: 20px;
}

.nav-btn {
  display: flex;
  align-items: center;
  gap: 8px;
  padding: 10px 16px;
  border: 1px solid #e5e7eb;
  background: white;
  border-radius: 6px;
  cursor: pointer;
  transition: all 0.2s ease;
  font-size: 14px;
  color: #374151;
  text-decoration: none;
}

.nav-btn:hover:not(:disabled) {
  background: #f9fafb;
  border-color: #10b981;
  color: #10b981;
}

.nav-btn:disabled {
  opacity: 0.5;
  cursor: not-allowed;
}

.current-lesson-info {
  text-align: center;
  flex: 1;
}

.lesson-title {
  margin: 0;
  font-size: 18px;
  font-weight: 600;
  color: #1f2937;
}

.lesson-type {
  font-size: 12px;
  color: #10b981;
  font-weight: 500;
  text-transform: uppercase;
  letter-spacing: 0.5px;
}

/* Content Body */
.content-body {
  flex: 1;
  padding: 30px;
  overflow-y: auto;
}

.learning-content {
  max-width: 800px;
  margin: 0 auto;
  line-height: 1.7;
}

.learning-content h1,
.learning-content h2,
.learning-content h3 {
  color: #1f2937;
  margin-top: 32px;
  margin-bottom: 16px;
}

.learning-content h1 {
  font-size: 28px;
  border-bottom: 2px solid #10b981;
  padding-bottom: 8px;
}

.learning-content h2 {
  font-size: 24px;
}

.learning-content h3 {
  font-size: 20px;
}

.learning-content p {
  margin-bottom: 16px;
  color: #374151;
}

.learning-content pre {
  background: #1f2937;
  color: #f9fafb;
  padding: 20px;
  border-radius: 8px;
  overflow-x: auto;
  margin: 24px 0;
  font-size: 14px;
  line-height: 1.5;
}

.learning-content code {
  background: #f3f4f6;
  color: #1f2937;
  padding: 2px 6px;
  border-radius: 4px;
  font-size: 13px;
}

.learning-content pre code {
  background: none;
  padding: 0;
  color: inherit;
}

.learning-content ol,
.learning-content ul {
  padding-right: 20px;
  margin-bottom: 16px;
}

.learning-content li {
  margin-bottom: 8px;
  color: #374151;
}

/* Content Actions */
.content-actions {
  background: #f8fafc;
  border-top: 1px solid #e5e7eb;
  padding: 20px 30px;
}

.action-buttons {
  display: flex;
  gap: 12px;
  justify-content: center;
}

/* Scope action button base styles to the action-buttons container to avoid affecting header icons */
.action-buttons .action-btn {
  display: flex;
  align-items: center;
  gap: 8px;
  padding: 12px 20px;
  border: none;
  border-radius: 6px;
  cursor: pointer;
  transition: all 0.2s ease;
  font-size: 14px;
  font-weight: 500;
}

.action-buttons .complete-btn {
  background: #10b981;
  color: white;
}

.action-buttons .complete-btn:hover {
  background: #059669;
}

.action-buttons .complete-btn.completed {
  background: #10b981;
  opacity: 0.8;
}

.action-buttons .note-btn {
  background: white;
  color: #6b7280;
  border: 1px solid #e5e7eb;
}

.action-buttons .note-btn:hover {
  background: #f9fafb;
  color: #374151;
}

/* Notes Panel */
.notes-panel {
  background: white;
  border-top: 1px solid #e5e7eb;
  padding: 20px 30px;
  max-height: 300px;
  overflow-y: auto;
}

.notes-header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 16px;
}

.notes-header h3 {
  margin: 0;
  color: #1f2937;
  font-size: 16px;
}

.close-notes {
  background: none;
  border: none;
  cursor: pointer;
  color: #9ca3af;
  padding: 4px;
}

.close-notes:hover {
  color: #6b7280;
}

.notes-content textarea {
  width: 100%;
  min-height: 120px;
  border: 1px solid #e5e7eb;
